{
  "name": "babel-skel",
  "version": "1.0.0",
  "description": "text",
  "main": "index.js",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/yakisuzu/babel-skel.git"
  },
  "scripts": {
    "eslint": "eslint src/",
    "babel": "babel src/ --out-dir dest/",
    "babel-watch": "babel src/ --out-dir dest/ --watch"
  },
  "author": "yakisuzu",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.13.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "rules": {
      "no-console": "warn"
    }
  }
}
